{
  "include": ["webilastik/", "tests/", "scripts/", "global_cache_impls/", "executor_getter_impls/", "scripts", "examples/"],
  "exclude": ["build/"],
  "strict": ["webilastik/datasource", "webilastik/datasink"],
  "typeCheckingMode": "basic",
  "reportMissingSuperCall": "error",
  "reportUnnecessaryTypeIgnoreComment":"error",
  "reportUninitializedInstanceVariable": "error",
  "reportUnknownMemberType": false,
  "reportIncompatibleMethodOverride": true,
  "reportMissingTypeArgument": true,
  "reportMissingTypeStubs": "warning",
  "useLibraryCodeForTypes": true,
  "extraPaths": ["executor_getter_impls/default/", "global_cache_impls/redis_cache/", "tests/", "scripts/"],
  "reportUnusedCallResult": "error",
  "reportUnusedVariable": "warning",
  "pythonVersion": "3.9",
  "pythonPlatform": "Linux"
}
